{
  "name": "whitelabel-token-mobile",
  "version": "1.0.0",
  "description": "Whitelabel Precious Metals Token Platform Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace WhitelabelToken.xcworkspace -scheme WhitelabelToken -configuration Release -destination generic/platform=iOS -archivePath WhitelabelToken.xcarchive archive",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "install:android": "react-native run-android --device-id=$(adb devices | grep -v List | tail -n +2 | head -n 1 | awk '{print $1}')",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "uninstall": "adb shell pm uninstall com.whitelabel.token"
  },
  "keywords": [
    "whitelabel",
    "react-native",
    "tokenization",
    "gold",
    "silver",
    "platinum",
    "mobile-app",
    "fintech",
    "jewellers",
    "blockchain"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.7",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "react-native-vector-icons": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^13.14.0",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-qrcode-generator": "^1.3.1",
    "react-native-camera": "^4.2.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-touch-id": "^4.4.1",
    "react-native-fingerprint-scanner": "^6.0.1",
    "react-native-fs": "^2.20.0",
    "react-native-share": "^9.4.1",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^3.9.2",
    "react-native-async-storage": "^1.19.3",
    "react-native-keychain": "^8.1.2",
    "react-native-secure-store": "^1.0.5",
    "react-native-background-job": "^1.0.4",
    "react-native-push-notification": "^8.1.1",
    "react-native-fcm": "^18.0.0",
    "@react-native-firebase/app": "^18.6.2",
    "@react-native-firebase/messaging": "^18.6.2",
    "@react-native-firebase/analytics": "^18.6.2",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-encrypted-storage-async": "^1.0.2",
    "react-native-crypto": "^2.2.0",
    "react-native-crypto-js": "^1.8.2",
    "react-native-jsonschema-form": "^1.8.0",
    "react-native-date-picker": "^4.2.14",
    "react-native-picker-select": "^8.1.0",
    "react-native-modal": "^13.0.1",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-swipeable-views": "^0.14.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-panorama-view": "^0.1.0",
    "react-native-fast-image": "^8.6.3",
